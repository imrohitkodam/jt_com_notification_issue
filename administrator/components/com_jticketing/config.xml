<!--Classroom training Settings-->
<fieldset name="classroom_training_settings" label="COM_JTICKETING_CLASSROOM_TRAINING_SETTING" description="COM_JTICKETING_CLASSROOM_TRAINING_SETTING_DESC" addfieldpath="/administrator/components/com_jticketing/assets/elements">

	<field name="enable_self_enrollment" class="btn-group" default="0" type="radio" label="COM_JTICKETING_ENABLE_SELF_ENROLLMENT" description="COM_JTICKETING_ENABLE_SELF_ENROLLMENT_DESC">
