<!--Waiting List Settings-->
<fieldset name="waiting_list_settings" label="COM_JTICKETING_WAITING_LIST_SETTING" description="COM_JTICKETING_WAITING_LIST_SETTING_DESC" addfieldpath="/administrator/components/com_jticketing/assets/elements">

	<field name="enable_waiting_list" class="btn-group" default="none" type="waitinglist" label="COM_JTICKETING_ENABLE_WAITING_LIST" description="COM_JTICKETING_ENABLE_WAITING_LIST_DESC"></field>

